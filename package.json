  "dependencies": {
    "@vivliostyle/cli": "^2.1.0",
    "del-cli": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "pagedjs": "^0.1.43",
    "pagedjs-cli": "^0.1.1"
