    "vb:wood": "vivliostyle build ./vivliostyle/samples/wood/index.html -o pdf/v-wood.pdf",
    "vb:candle": "vivliostyle build ./vivliostyle/samples/adaptive-layout/candle/index.xhtml -o pdf/v-candle.pdf",
    "vb:adaptive": "vivliostyle build ./vivliostyle/samples/adaptive-layout/adaptive/main.xhtml -o pdf/v-adaptive.pdf",
    "clean": "del \"pdf/*.pdf\"",
    "vivliostyle:build": "run-s clean vb:*",
    "pp:gon": "pagedjs-cli -d ./vivliostyle/samples/gon/index.html"
