    "vp:apollo8": "vivliostyle preview ./vivliostyle/samples/adaptive-layout/apollo8/index.xhtml",
    "vp:shirouma": "vivliostyle preview ./vivliostyle/samples/shirouma/index.html",
    "vp:cosmology": "vivliostyle preview ./cosmology/index.html",
    "vp:vivliostyle1": "vivliostyle preview -b ./vivliostyle/ja/vivliostyle-user-group-vol1/index.html",
    "vp:vivliostyle2": "vivliostyle preview -b ./vivliostyle/ja/vivliostyle-user-group-vol2/index.html",
