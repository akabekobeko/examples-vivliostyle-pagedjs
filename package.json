    "vb:shirouma": "vivliostyle build ./vivliostyle/samples/shirouma/index.html -o pdf/v-shirouma.pdf",
    "vb:cosmology": "vivliostyle build ./cosmology/index.html -o pdf/v-cosmology.pdf",
    "vb:vivliostyle1": "vivliostyle build -b ./vivliostyle/ja/vivliostyle-user-group-vol1/index.html -o pdf/v-vivliostyle1.pdf",
    "vb:vivliostyle2": "vivliostyle build -b ./vivliostyle/ja/vivliostyle-user-group-vol2/index.html -o pdf/v-vivliostyle2.pdf",
